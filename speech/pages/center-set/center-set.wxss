/* pages/center-set/center-set.wxss */
.set_head{
  width:100%;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.set_headBar{
  width:100%;
  border-bottom: 1rpx solid #e6e6e6;
  display: flex;
  justify-content: space-between;
  line-height: 88rpx;
  box-sizing: border-box;
  padding-right: 20rpx;
}
.set_headBarText{
  font-size: 30rpx;
  color: #333333;
}
.set_headBarRight{
  color: #999999;
  font-size: 28rpx;
}
.set_headBarImg{
  width: 14rpx;
	height: 24rpx;
  margin-top: 32rpx;
}
.set_headBarOut{
  width:100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  line-height: 88rpx;
  font-size: 30rpx;
  color: #ef9603;
}
.set_head .set_headBar:last-child{
  border: none;
}